<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#a7c4f2" />
      <stop offset="100%" stop-color="#6885d8" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-gradient)" />
  
  <!-- Healthcare Icons -->
  <g transform="translate(200, 150)" fill="#ffffff">
    <!-- Medical Cross -->
    <g transform="translate(-70, -20)">
      <rect x="-15" y="-40" width="30" height="80" rx="5" fill="#ffffff" />
      <rect x="-40" y="-15" width="80" height="30" rx="5" fill="#ffffff" />
    </g>
    
    <!-- Stethoscope -->
    <g transform="translate(20, 0)">
      <circle cx="-20" cy="-30" r="15" fill="#ffffff" />
      <circle cx="20" cy="-30" r="15" fill="#ffffff" />
      <path d="M-20,-15 C-20,20 20,20 20,-15" stroke="#ffffff" stroke-width="8" fill="none" />
      <path d="M0,20 L0,40" stroke="#ffffff" stroke-width="8" fill="none" />
    </g>
    
    <!-- Pill -->
    <g transform="translate(-20, 40)">
      <rect x="-20" y="-10" width="40" height="20" rx="10" fill="#ffffff" />
      <line x1="0" y1="-10" x2="0" y2="10" stroke="#6885d8" stroke-width="2" />
    </g>
  </g>
  
  <!-- Coming Soon Overlay -->
  <g transform="translate(200, 150)">
    <circle cx="0" cy="0" r="50" fill="none" stroke="#ffffff" stroke-width="4" />
    <text x="0" y="-10" font-family="Arial" font-size="16" font-weight="bold" fill="#ffffff" text-anchor="middle">COMING</text>
    <text x="0" y="15" font-family="Arial" font-size="16" font-weight="bold" fill="#ffffff" text-anchor="middle">SOON</text>
  </g>
</svg>