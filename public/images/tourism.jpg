<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#a7d9f2" />
      <stop offset="100%" stop-color="#68b0d8" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-gradient)" />
  
  <!-- Tourism Icons -->
  <g transform="translate(200, 150)" fill="#ffffff">
    <!-- Suitcase -->
    <rect x="-40" y="-20" width="30" height="40" rx="5" fill="#ffffff" />
    <rect x="10" y="-20" width="30" height="40" rx="5" fill="#ffffff" />
    <rect x="-50" y="-30" width="100" height="20" rx="5" fill="#ffffff" />
    <rect x="-10" y="-25" width="20" height="10" rx="2" fill="#68b0d8" />
    
    <!-- Eiffel Tower Simplified -->
    <g transform="translate(-80, -10) scale(0.7)">
      <path d="M0,80 L30,0 L60,80 L40,80 L40,100 L20,100 L20,80 Z" fill="#ffffff" />
      <rect x="25" y="30" width="10" height="5" fill="#68b0d8" />
      <rect x="20" y="50" width="20" height="5" fill="#68b0d8" />
    </g>
    
    <!-- Sun -->
    <g transform="translate(70, -30)">
      <circle cx="0" cy="0" r="15" fill="#ffffff" />
      <g stroke="#ffffff" stroke-width="3">
        <line x1="0" y1="-25" x2="0" y2="-18" />
        <line x1="0" y1="18" x2="0" y2="25" />
        <line x1="-25" y1="0" x2="-18" y2="0" />
        <line x1="18" y1="0" x2="25" y2="0" />
        <line x1="-18" y1="-18" x2="-13" y2="-13" />
        <line x1="18" y1="-18" x2="13" y2="-13" />
        <line x1="-18" y1="18" x2="-13" y2="13" />
        <line x1="18" y1="18" x2="13" y2="13" />
      </g>
    </g>
    
    <!-- Hat -->
    <g transform="translate(-20, 40)">
      <ellipse cx="0" cy="0" rx="25" ry="10" fill="#ffffff" />
      <ellipse cx="0" cy="-10" rx="15" ry="8" fill="#ffffff" />
      <rect x="-15" y="-10" width="30" height="2" fill="#68b0d8" />
    </g>
  </g>
</svg>